                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "status_code": {
                                                "type": "integer",
                                                "example": 200
                                            },
                                            "status_message": {
                                                "type": "string",
                                                "example": "Candidature rejetée"
                                            },
                                            "candidature": {
                                                "$ref": "#/components/schemas/Candidature"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
