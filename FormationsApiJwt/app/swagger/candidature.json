{
    "openapi": "3.0.0",
    "info": {
        "title": "My First API",
        "version": "0.1"
    },
    "paths": {
        "/api/candidatures": {
            "post": {
                "tags": [
                    "Candidatures"
                ],
                "summary": "Envoyer une candidature",
                "operationId": "ae13d3f262331c67269343834a7ed791",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nom_formation"
                                ],
                                "properties": {
                                    "nom_formation": {
                                        "type": "string",
                                        "example": "Nom de la formation"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Candidature envoyée avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "status_code": {
                                                "type": "integer",
                                                "example": 200
                                            },
                                            "status_message": {
                                                "type": "string",
                                                "example": "Candidature envoyée avec succès"
                                            },
                                            "candidature": {
                                                "$ref": "#/components/schemas/Candidature"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Vous avez déjà candidaté à cette formation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "status_code": {
                                                "type": "integer",
                                                "example": 403
                                            },
                                            "status_message": {
                                                "type": "string",
                                                "example": "Vous avez déjà candidaté à cette formation"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/candidatures/{id}/accept": {
            "put": {
                "tags": [
                    "Candidatures"
                ],
                "summary": "Accepter une candidature",
                "operationId": "687f3d67e190be25d976fed27e2b11bb",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la candidature",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Candidature acceptée",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "status_code": {
                                                "type": "integer",
                                                "example": 200
                                            },
                                            "status_message": {
                                                "type": "string",
                                                "example": "Candidature acceptée"
                                            },
                                            "candidature": {
                                                "$ref": "#/components/schemas/Candidature"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/candidatures/{id}/deny": {
            "put": {
                "tags": [
                    "Candidatures"
                ],
                "summary": "Rejeter une candidature",
                "operationId": "e15db8a2b08c2ff62e0af077cc867998",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la candidature",
                        "required": true,
